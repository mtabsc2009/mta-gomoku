<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>GoMokuMain</servlet-name>
        <servlet-class>goMoku.Controller.Web.GoMokuMain</servlet-class>
        <init-param>
            <param-name>GameType</param-name>
            <param-value>UC</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>GamePlay</servlet-name>
        <servlet-class>goMoku.Controller.Web.GamePlay</servlet-class>
        <init-param>
            <param-name>GameType</param-name>
            <param-value>UC</param-value>
        </init-param>
        </servlet>
    <servlet-mapping>
        <servlet-name>GoMokuMain</servlet-name>
        <url-pattern>/GoMokuMain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GamePlay</servlet-name>
        <url-pattern>/GamePlay</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
    </web-app>
